{% set cloudinaryPrefix = "https://res.cloudinary.com/nho/image/fetch/c_fill,f_auto,q_auto," %}
{% set logoUrl = "http://precious-prana.com/images/logo-precious-prana.png" %}
<a href="/" style="--aspect-ratio: 1.68"><img
  src="{{ cloudinaryPrefix }}w_360/{{ logoUrl }}"
  srcset="
    {{ cloudinaryPrefix }}w_360/{{ logoUrl }} 360w,
    {{ cloudinaryPrefix }}w_480/{{ logoUrl }} 480w,
    {{ cloudinaryPrefix }}w_640/{{ logoUrl }} 640w,
    {{ cloudinaryPrefix }}w_800/{{ logoUrl }} 800w"
  sizes="(min-width: 40rem) 20rem, 50vw"
  width="320"
  height="190.5"
  alt="Precious Prana" /></a>